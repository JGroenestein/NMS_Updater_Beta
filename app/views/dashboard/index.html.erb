<%# Flash Notices - Start %>
<% if flash[:notice] %>
  <div class="alert alert-success">
    <button type="button" class="close" data-dismiss="alert">
      <i class="icon-remove"></i>
    </button>

    <strong>
      <i class="icon-ok"></i>
      Success!
    </strong>

    <%= flash[:notice] %>
    <br>
  </div>
<% end %>

<% if flash[:alert] %>
  <div class="alert alert-error">
    <button type="button" class="close" data-dismiss="alert">
      <i class="icon-remove"></i>
    </button>

    <strong>
      <i class="icon-remove"></i>
      Oh snap!
    </strong>

    <%= flash[:alert] %>
    <br>
  </div>
<% end %>
<%# Flash Notices - End %>

